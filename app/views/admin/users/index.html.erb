<div class="row">
  <div class="span12">
    <table>
      <thead>
        <tr>
          <th>Name</th>
          <th></th>
        </tr>
      </thead>

      <tbody>
        <% @users.each do |user| %>
          <tr id="<%= dom_id(user) %>">
            <td><%= user.name %></td>
            <td>
              <%= link_to('Delete', admin_user_path(user),
                          method: :delete, remote: true,
                          data: { confirm: "Are you sure?"}) %>
            </td>
          </tr>
        <% end %>
      </tbody>
    </table>
  </div>
</div>